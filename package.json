{
  "name": "@svelte-plugins/datepicker",
  "version": "0.1.2",
  "license": "MIT",
  "description": "A simple datepicker component designed for Svelte.",
  "author": "Kieran Boyle (https://github.com/dysfunc)",
  "svelte": "./src/index.js",
  "types": "./src/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/svelte-plugins/datepicker.git"
  },
  "homepage": "https://github.com/svelte-plugins/datepicker",
  "bugs": "https://github.com/svelte-plugins/datepicker/issues",
  "keywords": [
    "datepicker",
    "rangepicker",
    "daterange",
    "timepicker",
    "calendar",
    "svelte",
    "components",
    "javascript"
  ],
  "files": [
    "/src",
    "/dist"
  ],
  "scripts": {
    "dev": "vite",
    "bundle": "vite build",
    "deploy": "npx gh-pages -d docs/build",
    "lint": "eslint -c ./.eslintrc.json --fix \"src/**/*.{.js,svelte}\"",
    "format": "prettier --write \"src/**/*.{js,json,svelte}\"",
    "test": "vitest --run --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/runtime": "^7.23.4",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/svelte": "^4.0.5",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.54.0",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-plugin-vitest": "^0.3.10",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "typescript": "^5.2.2",
    "vite": "^5.0.2",
    "vitest": "^0.34.6"
  },
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "svelte": "./src/index.js",
      "require": "./dist/index.js",
      "module": "./dist/index.mjs",
      "default": "./src/index.js"
    }
  }
}
